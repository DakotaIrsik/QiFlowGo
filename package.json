{
  "name": "qiflowgo",
  "version": "1.0.0",
  "description": "A mobile command center for managing multiple QiFlow AI swarm deployments across VMs, Raspberry Pis, and cloud instances.",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "lint": "tsc --noEmit",
    "version:patch": "npm version patch -m \"Release v%s\"",
    "version:minor": "npm version minor -m \"Release v%s\"",
    "version:major": "npm version major -m \"Release v%s\"",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DakotaIrsik/QiFlowGo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DakotaIrsik/QiFlowGo/issues"
  },
  "homepage": "https://github.com/DakotaIrsik/QiFlowGo#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "pg": "^8.16.3",
    "ssh2": "^1.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.5",
    "@types/ssh2": "^1.15.5",
    "@types/supertest": "^6.0.3",
    "ci-info": "^4.3.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "undici-types": "^7.16.0"
  }
}
